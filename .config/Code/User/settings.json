{
    "editor.fontFamily": "'Monaco', 'WenQuanYi Micro Hei', 'Noto Color Emoji'",
    "editor.fontSize": 16,
    "editor.fontLigatures": true,
    "editor.formatOnType": true,
    "editor.lineHeight": 25,
    "editor.renderIndentGuides": true,
    "editor.acceptSuggestionOnEnter": "off",
    "editor.dragAndDrop": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.minimap.enabled": false,
    "editor.snippetSuggestions": "inline",
    "editor.stablePeek": true,
    "terminal.integrated.fontFamily": "'Monaco', 'WenQuanYi Micro Hei Mono', 'Noto Color Emoji'",
    "terminal.integrated.fontSize": 15,
    "git.confirmSync": false,
    "window.menuBarVisibility": "toggle",
    "workbench.iconTheme": "vs-nomo-dark",
    "emmet.syntaxProfiles": {
        "vue-html": "html",
        "vue": "html"
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "vue"
    ],
    "files.insertFinalNewline": true,
    "explorer.openEditors.visible": 0,
    "diffEditor.renderSideBySide": false,
    "vetur.format.html.wrap_attributes": "force",
    "auto-close-tag.fullMode": true,
    "auto-close-tag.activationOnLanguage": [
        "html",
        "xml",
        "php",
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "plaintext",
        "markdown",
        "razor",
        "vue"
    ],
    "auto-rename-tag.activationOnLanguage": [
        "html",
        "xml",
        "php",
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "plaintext",
        "markdown",
        "razor",
        "vue"
    ],
    "git.enableSmartCommit": true,
    "gitlens.codeLens.enabled": false,
    "npm-intellisense.scanDevDependencies": true,
    "npm-intellisense.showBuildInLibs": true,
    "npm-intellisense.importLinebreak": ";\n"
}
