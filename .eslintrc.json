{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 10,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "rules": {
    "semi": "off",
    "no-console": "off",
    "no-cond-assign": "off",
    "no-fallthrough": "off",
    "no-debugger": "off",
    "no-case-declarations": "off"
  }
}